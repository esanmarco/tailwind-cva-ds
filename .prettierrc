    "printWidth": 90,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "prettier/prettier": [
        "error",
        {
            "singleQuote": true,
            "parser": "flow",
            "endOfLine": "auto"
        }
    ]
}
